{
    "name": "quantaforge/broadcasting",
    "description": "The QuantaForge Broadcasting package.",
        "license": "proprietary",
    "authors": [
        {
            "name": "Tomáš Selič (Smisch-DEV)",
            "email": "contact@smisch.sk"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/log": "^1.0|^2.0|^3.0",
        "quantaforge/bus": "^1.0.0",
        "quantaforge/collections": "^1.0.0",
        "quantaforge/container": "^1.0.0",
        "quantaforge/contracts": "^1.0.0",
        "quantaforge/queue": "^1.0.0",
        "quantaforge/support": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "QuantaForge\\Broadcasting\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "ext-hash": "Required to use the Ably and Pusher broadcast drivers.",
        "ably/ably-php": "Required to use the Ably broadcast driver (^1.0).",
        "pusher/pusher-php-server": "Required to use the Pusher broadcast driver (^6.0|^7.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
