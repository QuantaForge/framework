{
    "name": "quantaforge/console",
    "description": "The QuantaForge Console package.",
        "license": "proprietary",
    "authors": [
        {
            "name": "Tomáš Selič (Smisch-DEV)",
            "email": "contact@smisch.sk"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-mbstring": "*",
        "quantaforge/collections": "^1.0.0",
        "quantaforge/contracts": "^1.0.0",
        "quantaforge/macroable": "^1.0.0",
        "quantaforge/prompts": "^1.0.0",
        "quantaforge/support": "^1.0.0",
        "quantaforge/view": "^1.0.0",
        "quantaforge/termwind": "^1.0.0",
        "symfony/console": "^6.2",
        "symfony/process": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "QuantaForge\\Console\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "ext-pcntl": "Required to use signal trapping.",
        "dragonmantank/cron-expression": "Required to use scheduler (^3.3.2).",
        "guzzlehttp/guzzle": "Required to use the ping methods on schedules (^7.5).",
        "quantaforge/bus": "Required to use the scheduled job dispatcher (^10.0).",
        "quantaforge/container": "Required to use the scheduler (^10.0).",
        "quantaforge/filesystem": "Required to use the generator command (^10.0).",
        "quantaforge/queue": "Required to use closures for scheduled jobs (^10.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
